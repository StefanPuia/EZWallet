<!DOCTYPE html>
<html>
	<%- include("partials/head") %>
	<body>
		<%- include("partials/header") %>
		<main>
            <div class="container center">
                <h5>Records</h5>
                <div class="row">
                        <div class="input-field col s4">
                            <select id="sMonth">
   								<option selected value='1'>Janaury</option>
   								<option value='2'>February</option>
   								<option value='3'>March</option>
   								<option value='4'>April</option>
   								<option value='5'>May</option>
   								<option value='6'>June</option>
   								<option value='7'>July</option>
   								<option value='8'>August</option>
   								<option value='9'>September</option>
   								<option value='10'>October</option>
   								<option value='11'>November</option>
   								<option value='12'>December</option>
							</select>
                            <label for="textarea1">Month</label>
                        </div>

                        <div class="input-field col s4">
                            <input id="sYear" type="number" class="datepicker">
                            <label for="textarea1">Year</label>
                        </div>

						<div class="input-field col s4">
                            <select id="transaction_category">
                                <option value="" disabled selected>Select a category</option>
                            </select>
                            <label>Category</label>
                        </div>

                        <div class="input-field col s6">
                            <input id="sText" type="text" class="datepicker">
                            <label ></label>
                        </div>
                        <a class="waves-effect waves-light btn col s6">Search</a>

				<div class="row">
					<div class="col s12">
						<h5>Results</h5>
						<ul id="recordList" class="collection left-align black-text">
							<li class="collection-item">
								<a href="/records" class="black-text">
									Show More...
									<span class="secondary-content">
										<i class="material-icons">send</i>
									</span>
								</a>
							</li>
						</ul>
					</div>

                </div>
            </div>
		</main>
	</body>
</html>
